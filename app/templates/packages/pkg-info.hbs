<h1 class="short-header">{{model.name}} <small>{{model.title}}</small></h1>
<dl class="dl-horizontal">
  <dt>Version</dt>
  <dd>{{model.version}}</dd>
  <dt>Description</dt>
  <dd>{{removeUnicode model.description}}</dd>
  <dt>Publication Date</dt>
  <dd>{{moment-format model.publicationDate "MM/DD/YYYY"}}</dd>
</dl>
